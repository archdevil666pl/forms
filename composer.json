{
    "name": "vegas-cmf/forms",
    "type": "library",
    "description": "Vegas CMF Forms",
    "license": "MIT",
    "authors": [
        {
            "name": "Amsterdam Standard Vegas Team",
            "email": "vegas@amsterdam-standard.pl"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/vegas-cmf/assets.git"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-phalcon": "2.*",
        "vegas-cmf/assets" : "dev-master",
        "vegas-cmf/validation" : "2.*",
        "vegas-cmf/filter" : "2.*"
    },
    "require-dev": {
        "phalcon/devtools": "2.0.x-dev",
        "phpunit/phpunit": "4.0.*",
        "satooshi/php-coveralls": "1.0.1"
    },
    "autoload": {
        "psr-4": {
            "Vegas\\Forms\\": "src/",
            "Vegas\\Tests\\": "tests/"
        }
    }
}
